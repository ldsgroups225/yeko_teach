# Supabase Configuration (existing)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth Configuration (new)
# Web Client ID from Google Cloud Console (required for both Android and iOS)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id.apps.googleusercontent.com

# iOS Client ID from Google Cloud Console (optional, for iOS-specific features)
EXPO_PUBLIC_IOS_CLIENT_ID="com.googleusercontent.apps._some_id_here_"

# Android configuration is handled via google-services.json file
# Place your google-services.json file in the project root

# For development
EXPO_PUBLIC_SITE_URL=http://localhost:8081

# Local db
DB_FILE_NAME=your_table_name
